/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
.jiantou{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.jiantou::after{
  content: "";
  height: 15rpx;
  width: 15rpx;
  border-top: 3rpx solid #ccc;
  border-right: 3rpx solid #ccc;
  transform: rotate(45deg);
}
